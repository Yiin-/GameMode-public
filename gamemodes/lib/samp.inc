SetPlayerFacePlayer(pid, giveplayerid) {
	new
		Float: pX,
		Float: pY,
		Float: pZ,
		Float: gX,
		Float: gY,
		Float: gZ
	;
	if(GetPlayerPos(pid, pX, pY, pZ) && GetPlayerPos(giveplayerid, gX, gY, gZ)) {
		SetPlayerFacingAngle(pid, (pX = -atan2((gX - pX), (gY - pY))));
		return 1;
	}
	return 0;
}

stock Float:GetPlayerSpeed(playerid, Float:fakeX = -1000.0, Float:fakeY = -1000.0, Float:fakeZ = -1000.0)
{
	static Float:ST[4];

	if(IsPlayerInAnyVehicle(playerid))
		GetVehicleVelocity(GetPlayerVehicleID(playerid),ST[0],ST[1],ST[2]);
	else
		GetPlayerVelocity(playerid,ST[0],ST[1],ST[2]);

	if(fakeX != -1000.0) ST[0] = fakeX;
	if(fakeY != -1000.0) ST[1] = fakeY;
	if(fakeZ != -1000.0) ST[2] = fakeZ;

	return floatsqroot(floatpower(ST[0], 2.0) + floatpower(ST[1], 2.0) + floatpower(ST[2], 2.0)) * 170.0;
}

#if !defined IsPlayerSpawned
stock IsPlayerSpawned(playerid) {
	return GetPlayerState(playerid) == PLAYER_STATE_ONFOOT
			|| GetPlayerState(playerid) == PLAYER_STATE_DRIVER
			|| GetPlayerState(playerid) == PLAYER_STATE_SPAWNED
			|| GetPlayerState(playerid) == PLAYER_STATE_PASSENGER;
}
#endif

#if !defined GetWeaponSlot
stock GetWeaponSlot(weaponid) {
	static const
		s_weaponslots[] = {
			0x00000101, 0x01010101, 0x01010A0A, 0x0A0A0A0A,
			0x080808FF, 0xFFFF0202, 0x02030303, 0x04040505,
			0x04060607, 0x07070708, 0x0C090909, 0x0B0B0B00
		}
	;
	
	if (0 <= weaponid <= 46)
		return s_weaponslots{weaponid} == 0xFF ? WEAPONSLOT_NONE : s_weaponslots{weaponid};
	
	return WEAPONSLOT_NONE;
}
#endif

